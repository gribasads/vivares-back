{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/auth/(.*)",
            "dest": "/api/auth"
        },
        {
            "src": "/api/users/(.*)",
            "dest": "/api/users"
        }
    ]
} 